{
	"compilerOptions": {
		"lib": [ "ES2020" ],
		"target": "ES2020",
		"module": "Node16",
		"strict": true,
		"allowJs": true,
		"checkJs": false,
		"charset": "utf-8",
		"sourceMap": false,
		"declaration": true,
		"alwaysStrict": true,
		"moduleResolution": "Node16",
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictPropertyInitialization": true,
		"forceConsistentCasingInFileNames": true
	},
	"files": [ "./fetch.ts" ],
	"compileOnSave": true,
	"buildOptions": {
		"verbose": true
	}
}